<!--version="1.0" encoding="utf-8" standalone="yes"-->
<window>
  <id>10</id>
  <defaultcontrol>2</defaultcontrol>
  <allowoverlay>control.isvisible(13371337)</allowoverlay>
  <define>#header.label:213</define>
  <define>#header.background:hover_settings.png</define>
  <animation effect="fade" time="250" start="100" end="100">WindowClose</animation>
  <controls>
    <import>common.window.background.xml</import>
    <import>common.window.overlay.xml</import>
    <control>
      <description>calibration image</description>
      <type>image</type>
      <id>1</id>
      <width>1920</width>
      <height>1080</height>
      <texture>settingsUICalibration.png</texture>
      <animation effect="fade" time="250">WindowClose</animation>
      <animation effect="fade" time="250">WindowOpen</animation>
    </control>
    <control>
      <description>rectangle image</description>
      <type>image</type>
      <id>1</id>
      <posX>48</posX>
      <posY>505</posY>
      <texture>-</texture>
      <animation effect="fade" time="250">WindowClose</animation>
      <animation effect="fade" time="250">WindowOpen</animation>
    </control>
    <control>
      <description>coordinates label</description>
      <type>label</type>
      <id>2</id>
      <posX>0</posX>
      <posY>552</posY>
      <label>5</label>
      <font>font12</font>
      <align>center</align>
      <width>1920</width>
      <textcolor>#ColorAlternate</textcolor>
      <animation effect="fade" time="250">WindowClose</animation>
      <animation effect="fade" time="250">WindowOpen</animation>
    </control>
  </controls>
</window>