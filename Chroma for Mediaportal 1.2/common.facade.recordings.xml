			<control>
				<description>composite control consisting of a list control and a thumbnail panel</description>
				<type>facadeview</type>
				<id>50</id>
				<control>
					<description>listcontrol</description>
					<type>listcontrol</type>
					<id>50</id>
					<onup>50</onup>
					<ondown>50</ondown>
					<onleft>2</onleft>
					<onright>2</onright>
					<!--<visible>control.isvisible(745381)</visible>
					<animation effect="fade" time="200">visible</animation>-->
				</control>
				<control>
					<description>album view listcontrol</description>
					<type>listcontrol</type>
					<subtype>album</subtype>
					<id>50</id>
					<onup>50</onup>
					<ondown>50</ondown>
					<onleft>2</onleft>
					<onright>2</onright>
					<textYOff>4</textYOff>
					<textYOff2>51</textYOff2>
					<textYOff3>51</textYOff3>
					<textXOff2>0</textXOff2>
					<textXOff3>207</textXOff3>
					<PinIconYOff>25</PinIconYOff>
					<font2>font12</font2>
					<font3>font12</font3>
					<textcolor2>#ColorMainSub</textcolor2>
					<textcolor3>#ColorMainSub</textcolor3>
					<itemWidth>122</itemWidth>
					<itemHeight>92</itemHeight>
					<textureFocus>background_focus2.png</textureFocus>
					<textureHeight>100</textureHeight>
					<!--<visible>control.isvisible(745387)</visible>
					<animation effect="fade" time="200">visible</animation>-->
				</control>
				<control>
					<description>Thumbnail Panel</description>
					<type>thumbnailpanel</type>
					<id>50</id>
					<onup>2</onup>
					<ondown>2</ondown>
					<onleft>2</onleft>
					<onright>2</onright>
					<posX>80</posX>
					<posY>380</posY>
					<width>1410</width>
					<height>620</height>
					<scrollOffset>1</scrollOffset>
					<!-- SMALL -->
					<itemWidth>266</itemWidth>
					<itemHeight>150</itemHeight>
					<textureWidth>266</textureWidth>
					<textureHeight>150</textureHeight>
					<thumbWidth>266</thumbWidth>
					<thumbHeight>150</thumbHeight>
					<!-- LARGE -->
					<itemWidthBig>533</itemWidthBig>
					<itemHeightBig>300</itemHeightBig>
					<thumbWidthBig>533</thumbWidthBig>
					<thumbHeightBig>300</thumbHeightBig>
					<textureWidthBig>533</textureWidthBig>
					<textureHeightBig>300</textureHeightBig>
					<!--<visible>control.isvisible(745382)|control.isvisible(745383)</visible>
					<animation effect="fade" time="200">visible</animation>-->
					<scrollbarXOff>-1520</scrollbarXOff>
				</control>
				<control>
					<description>Filmstrip view</description>
					<type>filmstrip</type>
					<id>50</id>
					<onup>2</onup>
					<ondown>2</ondown>
					<onleft>50</onleft>
					<onright>50</onright>
					<posX>50</posX>
					<posY>636</posY>
					<height>328</height>
					<thumbWidth>364</thumbWidth>
					<thumbHeight>204</thumbHeight>
					<itemWidth>364</itemWidth>
					<itemHeight>204</itemHeight>
					<textureWidth>364</textureWidth>
					<textureHeight>204</textureHeight>
					<scrollbarYOff>254</scrollbarYOff>
					<!--<visible>control.isvisible(745384)</visible>
					<animation effect="fade" time="200">visible</animation>-->
				</control>

				<control>
					<description>Cover Flow view</description>
					<type>coverflow</type>
					<id>50</id>
					<onup>2</onup>
					<ondown>2</ondown>
					<onleft>50</onleft>
					<onright>50</onright>
					<posY>676</posY>
					<cardWidth>364</cardWidth>
					<cardHeight>204</cardHeight>
					<frameWidth>364</frameWidth>
					<frameHeight>204</frameHeight>
					<scrollbarYOff>214</scrollbarYOff>
					<keepaspectratio>yes</keepaspectratio>
					<!--<visible>control.isvisible(745385)</visible>
					<animation effect="fade" time="200">visible</animation>-->
					<subitems>
						<subitem>
							<![CDATA[
							<controls>
								
								<control>
									<description>background</description>
									<type>image</type>
									<posX>0</posX>
									<posY>0</posY>
									<width>364</width>
									<height>204</height>
									<texture>black.png</texture>
								</control>
								
								<control>
									<description>Title</description>
									<type>fadelabel</type>
									<id>0</id>
									<posX>25</posX>
									<posY>25</posY>
									<width>314</width>
									<label>#selecteditem</label>
									<font>font12</font>
									<textcolor>#ColorMain</textcolor>
									<wrapString> ·· </wrapString>
								</control>
								<control>
									<description>genre</description>
									<type>fadelabel</type>
									<posX>25</posX>
									<posY>75</posY>
									<width>314</width>
									<label>#selecteditem2</label>
									<font>font12</font>
									<textcolor>#ColorAlternate</textcolor>
									<wrapString> ·· </wrapString>
								</control>
								
							</controls>
							]]>
						</subitem>
					</subitems>
				</control>
				<control>
					<description>playlist listcontrol</description>
					<type>playlistcontrol</type>
					<id>50</id>
					<onup>2</onup>
					<ondown>2</ondown>
					<onleft>2</onleft>
					<onright>2</onright>
					<!--<visible>control.isvisible(745386)</visible>
					<animation effect="fade" time="200" condition="!control.hasfocus(50)">visible</animation>-->
				</control>
			</control>
