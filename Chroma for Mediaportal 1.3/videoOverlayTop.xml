<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<!--version="1.0" encoding="utf-8" standalone="yes"-->
<window>
	<defaultcontrol>4</defaultcontrol>
	<id>3008</id>
	<controls>
		<control>
			<description>Pause logo</description>
			<type>image</type>
			<id>4</id>
			<posX>100</posX>
			<posY>960</posY>
			<width>50</width>
			<height>50</height>
			<texture>osd_pause.png</texture>
			<visible>player.paused</visible>
			<animation effect="fade" time="100">visible</animation>
			<animation effect="slide" tween="circle" easing="out" time="200" start="0,500" reversible="false">WindowOpen</animation>
			<animation effect="slide" tween="circle" easing="out" time="200" end="0,500" reversible="false">WindowClose</animation>
		</control>
		<control>
			<description>FF logo</description>
			<type>image</type>
			<id>5</id>
			<posX>100</posX>
			<posY>960</posY>
			<width>50</width>
			<height>50</height>
			<texture>osd_forward.png</texture>
			<visible>player.forwarding</visible>
			<animation effect="fade" time="100">visible</animation>
			<animation effect="slide" tween="circle" easing="out" time="200" start="0,500" reversible="false">WindowOpen</animation>
			<animation effect="slide" tween="circle" easing="out" time="200" end="0,500" reversible="false">WindowClose</animation>
		</control>
		<control>
			<description>RW logo</description>
			<type>image</type>
			<id>6</id>
			<posX>100</posX>
			<posY>960</posY>
			<width>50</width>
			<height>50</height>
			<texture>osd_rew.png</texture>
			<visible>player.rewinding</visible>
			<animation effect="fade" time="100">visible</animation>
			<animation effect="slide" tween="circle" easing="out" time="200" start="0,500" reversible="false">WindowOpen</animation>
			<animation effect="slide" tween="circle" easing="out" time="200" end="0,500" reversible="false">WindowClose</animation>
		</control>
	</controls>
</window>