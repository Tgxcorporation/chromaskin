<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<!--version="1.0" encoding="utf-8" standalone="yes"-->
<window>
	<id>9811</id>
	<defaultcontrol>50</defaultcontrol>
	<allowoverlay>player.hasmedia</allowoverlay>
	<define>#header.title:1300</define>
	<define>#header.subtitle:#TVSeries.SimpleCurrentView / #TVSeries.Series.Pretty_Name / #itemcount #(L(507))</define>
	<define>#header.background:hover_my tv.png</define>
	<controls>
		<import>TVSeries.dummy.xml</import>
		<import>common.window.background.xml</import>
		<control>
			<description>Fanart Background #1</description>
			<type>image</type>
			<id>524</id>
			<posX>0</posX>
			<posY>0</posY>
			<width>1920</width>
			<height>1080</height>
			<texture>#TVSeries.Fanart.1</texture>
			<animation effect="fade" time="250">visible</animation>
			<animation effect="fade" time="250" delay="250">hidden</animation>
			<animation effect="fade" time="1">WindowClose</animation>
			<animation effect="fade" time="250">WindowOpen</animation>
			<visible>no</visible>
			<!-- Do not modify the visibility for this component. It is handled by the plug-in. -->
		</control>
		<control>
			<description>Fanart Background #2</description>
			<type>image</type>
			<id>525</id>
			<posX>0</posX>
			<posY>0</posY>
			<width>1920</width>
			<height>1080</height>
			<texture>#TVSeries.Fanart.2</texture>
			<animation effect="fade" time="250">visible</animation>
			<animation effect="fade" time="250" delay="250">hidden</animation>
			<animation effect="fade" time="1">WindowClose</animation>
			<animation effect="fade" time="250">WindowOpen</animation>
			<visible>no</visible>
			<!-- Do not modify the visibility for this component. It is handled by the plug-in. -->
		</control>
		<import>common.window.header.xml</import>
		<import>TVSeries.Facade.xml</import>
		<import>TVSeries.GroupList.xml</import>
		<import>TVSeries.SeriesAndSeasonListPostersWideBanners.xml</import>
		<import>TVSeries.SeriesAndSeasonFilmstrip.xml</import>
		<import>TVSeries.EpisodeList.xml</import>
		<import>TVSeries.HiddenMenu.xml</import>

		<control>
			<description>DUMMY BUTTON FOR FANART</description>
			<type>button</type>
			<id>53</id>
			<posX>1640</posX>
			<posY>0</posY>
			<width>650</width>
			<height>1080</height>
			<onright>50</onright>
			<onleft>50</onleft>
			<ondown>50</ondown>
			<onup>17</onup>
			<textureFocus>-</textureFocus>
			<textureNoFocus>-</textureNoFocus>
			<visible>!control.isvisible(1240)</visible>
		</control>

		<!-- IMPORT PROCESS ANIMATION -->
		<control>
			<description>Import Animation</description>
			<type>animation</type>
			<id>51</id>
			<posX>888</posX>
			<posY>468</posY>
			<width>144</width>
			<height>144</height>
			<textures>common.waiting.1.png;common.waiting.2.png;common.waiting.3.png;common.waiting.4.png;common.waiting.5.png;common.waiting.6.png;common.waiting.7.png;common.waiting.8.png</textures>
			<Duration>0:0:01.5</Duration>
			<keepaspectratio>yes</keepaspectratio>
		</control>
	</controls>
</window>